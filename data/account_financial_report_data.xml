<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Financial Reports -->
        <record id="account_financial_report_profitandloss0" model="account.report">
            <field name="name">Profit and Loss</field>
            <field name="root_report_id" ref="account.profit_and_loss"/>
            <field name="filter_date_range" eval="True"/>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_journals" eval="True"/>
            <field name="filter_multi_company" eval="True"/>
            <field name="filter_analytic_groupby" eval="True"/>
            <field name="filter_comparison" eval="True"/>
        </record>

        <record id="account_financial_report_balancesheet0" model="account.report">
            <field name="name">Balance Sheet</field>
            <field name="root_report_id" ref="account.balance_sheet"/>
            <field name="filter_date_range" eval="True"/>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_journals" eval="True"/>
            <field name="filter_multi_company" eval="True"/>
            <field name="filter_comparison" eval="True"/>
        </record>

        <record id="account_financial_report_cashflow0" model="account.report">
            <field name="name">Cash Flow Statement</field>
            <field name="root_report_id" ref="account.cash_flow_report"/>
            <field name="filter_date_range" eval="True"/>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_journals" eval="True"/>
            <field name="filter_multi_company" eval="True"/>
            <field name="filter_comparison" eval="True"/>
        </record>

        <record id="account_financial_report_partner_ledger0" model="account.report">
            <field name="name">Partner Ledger</field>
            <field name="root_report_id" ref="account.partner_ledger"/>
            <field name="filter_date_range" eval="True"/>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_journals" eval="True"/>
            <field name="filter_multi_company" eval="True"/>
            <field name="filter_partner" eval="True"/>
            <field name="filter_account_type" eval="True"/>
        </record>

        <record id="account_financial_report_general_ledger0" model="account.report">
            <field name="name">General Ledger</field>
            <field name="root_report_id" ref="account.general_ledger"/>
            <field name="filter_date_range" eval="True"/>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_journals" eval="True"/>
            <field name="filter_multi_company" eval="True"/>
            <field name="filter_account_type" eval="True"/>
            <field name="filter_comparison" eval="True"/>
        </record>

        <record id="account_financial_report_trial_balance0" model="account.report">
            <field name="name">Trial Balance</field>
            <field name="root_report_id" ref="account.trial_balance"/>
            <field name="filter_date_range" eval="True"/>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_journals" eval="True"/>
            <field name="filter_multi_company" eval="True"/>
            <field name="filter_account_type" eval="True"/>
            <field name="filter_comparison" eval="True"/>
        </record>

        <record id="account_financial_report_aged_receivable0" model="account.report">
            <field name="name">Aged Receivable</field>
            <field name="root_report_id" ref="account.aged_receivable"/>
            <field name="filter_date_range" eval="True"/>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_journals" eval="True"/>
            <field name="filter_multi_company" eval="True"/>
            <field name="filter_partner" eval="True"/>
        </record>

        <record id="account_financial_report_aged_payable0" model="account.report">
            <field name="name">Aged Payable</field>
            <field name="root_report_id" ref="account.aged_payable"/>
            <field name="filter_date_range" eval="True"/>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_journals" eval="True"/>
            <field name="filter_multi_company" eval="True"/>
            <field name="filter_partner" eval="True"/>
        </record>

        <!-- Balance Sheet -->
        <record id="account_financial_report_bs" model="account.financial.report">
            <field name="name">Balance Sheet</field>
            <field name="sequence">1</field>
            <field name="sign">1</field>
            <field name="display_detail">no_detail</field>
            <field name="type">sum</field>
            <field name="style_overwrite">1</field>
        </record>

        <!-- Assets -->
        <record id="account_financial_report_assets" model="account.financial.report">
            <field name="name">Assets</field>
            <field name="sequence">10</field>
            <field name="parent_id" ref="account_financial_report_bs"/>
            <field name="display_detail">detail_flat</field>
            <field name="type">sum</field>
            <field name="style_overwrite">2</field>
            <field name="sign">1</field>
        </record>

        <!-- Fixed Assets -->
        <record id="account_financial_report_fixed_assets" model="account.financial.report">
            <field name="name">Fixed Assets</field>
            <field name="sequence">100</field>
            <field name="parent_id" ref="account_financial_report_assets"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_fixed_assets'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">1</field>
        </record>

        <!-- Current Assets -->
        <record id="account_financial_report_current_assets" model="account.financial.report">
            <field name="name">Current Assets</field>
            <field name="sequence">110</field>
            <field name="parent_id" ref="account_financial_report_assets"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_current_assets'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">1</field>
        </record>

        <!-- Bank and Cash -->
        <record id="account_financial_report_bank_cash" model="account.financial.report">
            <field name="name">Bank and Cash</field>
            <field name="sequence">120</field>
            <field name="parent_id" ref="account_financial_report_assets"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_liquidity'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">1</field>
        </record>

        <!-- Receivables -->
        <record id="account_financial_report_receivables" model="account.financial.report">
            <field name="name">Receivables</field>
            <field name="sequence">130</field>
            <field name="parent_id" ref="account_financial_report_assets"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_receivable'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">1</field>
        </record>

        <!-- Liabilities -->
        <record id="account_financial_report_liabilities" model="account.financial.report">
            <field name="name">Liabilities</field>
            <field name="sequence">20</field>
            <field name="parent_id" ref="account_financial_report_bs"/>
            <field name="display_detail">detail_flat</field>
            <field name="type">sum</field>
            <field name="style_overwrite">2</field>
            <field name="sign">1</field>
        </record>

        <!-- Current Liabilities -->
        <record id="account_financial_report_current_liabilities" model="account.financial.report">
            <field name="name">Current Liabilities</field>
            <field name="sequence">210</field>
            <field name="parent_id" ref="account_financial_report_liabilities"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_current_liabilities'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">1</field>
        </record>

        <!-- Payables -->
        <record id="account_financial_report_payables" model="account.financial.report">
            <field name="name">Payables</field>
            <field name="sequence">220</field>
            <field name="parent_id" ref="account_financial_report_liabilities"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_payable'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">1</field>
        </record>

        <!-- Profit and Loss -->
        <record id="account_financial_report_pl" model="account.financial.report">
            <field name="name">Profit and Loss</field>
            <field name="sequence">2</field>
            <field name="sign">1</field>
            <field name="display_detail">no_detail</field>
            <field name="type">sum</field>
            <field name="style_overwrite">1</field>
        </record>

        <!-- Income -->
        <record id="account_financial_report_income" model="account.financial.report">
            <field name="name">Income</field>
            <field name="sequence">310</field>
            <field name="parent_id" ref="account_financial_report_pl"/>
            <field name="display_detail">detail_flat</field>
            <field name="type">sum</field>
            <field name="style_overwrite">2</field>
            <field name="sign">1</field>
        </record>

        <!-- Operating Income -->
        <record id="account_financial_report_operating_income" model="account.financial.report">
            <field name="name">Operating Income</field>
            <field name="sequence">320</field>
            <field name="parent_id" ref="account_financial_report_income"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_revenue'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">1</field>
        </record>

        <!-- Expenses -->
        <record id="account_financial_report_expense" model="account.financial.report">
            <field name="name">Expenses</field>
            <field name="sequence">330</field>
            <field name="parent_id" ref="account_financial_report_pl"/>
            <field name="display_detail">detail_flat</field>
            <field name="type">sum</field>
            <field name="style_overwrite">2</field>
            <field name="sign">-1</field>
        </record>

        <!-- Operating Expenses -->
        <record id="account_financial_report_operating_expense" model="account.financial.report">
            <field name="name">Operating Expenses</field>
            <field name="sequence">340</field>
            <field name="parent_id" ref="account_financial_report_expense"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_expenses'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">-1</field>
        </record>

        <!-- Depreciation -->
        <record id="account_financial_report_depreciation" model="account.financial.report">
            <field name="name">Depreciation</field>
            <field name="sequence">350</field>
            <field name="parent_id" ref="account_financial_report_expense"/>
            <field name="type">account_type</field>
            <field name="account_type_ids" eval="[(4, ref('account.data_account_type_depreciation'))]"/>
            <field name="style_overwrite">3</field>
            <field name="sign">-1</field>
        </record>
    </data>
</odoo>
